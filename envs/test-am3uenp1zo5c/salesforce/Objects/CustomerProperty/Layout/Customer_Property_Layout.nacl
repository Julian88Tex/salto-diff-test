salesforce.Layout CustomerProperty_Customer_Property_Layout@bss {
  excludeButtons = [
    "Submit",
  ]
  layoutSections = [
    {
      customLabel = false
      detailHeading = true
      editHeading = true
      label = "Information"
      layoutColumns = [
        {
          layoutItems = [
            {
              behavior = "Required"
              field = salesforce.CustomerProperty.field.PrimaryOwnerId
            },
            {
              behavior = "Edit"
              field = salesforce.CustomerProperty.field.PurchaseDate
            },
            {
              behavior = "Edit"
              field = salesforce.CustomerProperty.field.PrimaryUse
            },
          ]
        },
        {
          layoutItems = [
            {
              behavior = "Edit"
              field = salesforce.CustomerProperty.field.FairMarketValue
            },
            {
              behavior = "Edit"
              field = salesforce.CustomerProperty.field.PurchasePrice
            },
            {
              behavior = "Edit"
              field = salesforce.CustomerProperty.field.Address
            },
          ]
        },
      ]
      style = "TwoColumnsLeftToRight"
    },
    {
      customLabel = false
      detailHeading = false
      editHeading = false
      label = "Custom Links"
      style = "CustomLinks"
    },
  ]
  showEmailCheckbox = false
  showRunAssignmentRulesCheckbox = false
  showSubmitAndAttachButton = false
  fullName = "CustomerProperty-Customer Property Layout"
  _parent = [
    salesforce.CustomerProperty,
  ]
  _alias = "CustomerProperty-Customer Property Layout"
}
